{
  "main": "./dist/map.common.js",
  "name": "@hungpv97/vue-library-map",
  "exports": {
    ".": "./dist/map.common.js",
    "./*": "./dist/*",
    "./mixin": "./dist/mixin.common.js",
    "./store": "./dist/store.common.js",
    "./helper": "./dist/helper.common.js",
    "./reader": "./dist/reader.common.js",
    "./event-bus": "./dist/event-bus.common.js",
    "./main.css": "./dist/map.css"
  },
  "author": {
    "name": "hung.pv"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hung4564/vue-library-map"
  },
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bugs": "https://github.com/hung4564/vue-library-map/issues",
  "license": "MIT",
  "version": "0.0.0",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "web": "NODE_ENV=webproduction vue-cli-service build",
    "deploy": "node scripts/gh-pages-deploy.js",
    "build": "npm-run-all --parallel build-app build-mixin build-store build-helper build-reader build-model build-event-bus",
    "build-app": "vue-cli-service build --target lib --no-clean --name map build/app.js",
    "build-event-bus": "vue-cli-service build --target lib --no-clean --name event-bus build/event-bus.js",
    "build-mixin": "vue-cli-service build --target lib --no-clean --name mixin build/mixin.js",
    "build-model": "vue-cli-service build --target lib --no-clean --name model build/model.js",
    "build-store": "vue-cli-service build --target lib --no-clean --name store build/store.js",
    "build-helper": "vue-cli-service build --target lib --no-clean --name helper build/helper.js",
    "build-reader": "vue-cli-service build --target lib --no-clean --name reader build/reader.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@hungpv97/vue-library-draggable": "^1.0.0",
    "@jamescoyle/vue-icon": "^0.1.2",
    "@linusborg/vue-simple-portal": "^0.1.5",
    "@mapbox/mapbox-gl-sync-move": "^0.3.1",
    "@mdi/js": "^7.2.96",
    "@turf/bbox": "^6.5.0",
    "core-js": "3.29.0",
    "file-saver": "2.0.5",
    "geojson-validation": "^1.0.2",
    "lodash": "^4.17.21",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "mapbox-gl": "1.13.0",
    "minify-css-string": "^1.0.0",
    "mitt": "^3.0.0",
    "papaparse": "^5.4.1",
    "proj4": "2.9.0",
    "sass": "1.59.2",
    "sass-loader": "13.2.0",
    "v-click-outside": "^3.2.0",
    "vue": "2.7.14",
    "vue-drag-resize": "1.5.4",
    "vue-resize-directive": "1.2.0",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@babel/eslint-parser": "7.19.1",
    "@vue/cli-plugin-babel": "5.0.8",
    "@vue/cli-plugin-eslint": "5.0.8",
    "@vue/cli-service": "5.0.8",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "8.0.3",
    "execa": "^7.1.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.4",
    "vue-template-compiler": "2.7.14"
  }
}
